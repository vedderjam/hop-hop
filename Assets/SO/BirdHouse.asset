  - name: "GOLONDRINA COM\xDAN"
    sprite: {fileID: 21300000, guid: e85ee415cbdc69346bffdb79042aa198, type: 3}
    prize: 0
    purchased: 1
    prefab: {fileID: 8501833117856623768, guid: ed29cba782eb46d45944fa9edd1579bf,
